    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/cmstead/contractify/issues"
    },
    "homepage": "https://github.com/cmstead/contractify#readme",
    "devDependencies": {
        "chai": "^4.2.0",
        "mocha": "^7.0.1",
        "nanocommit": "^1.14.0"
    },
    "nanocommit": {
        "testCommand": "node ./node_modules/mocha/bin/mocha ./test/*.test.js",
